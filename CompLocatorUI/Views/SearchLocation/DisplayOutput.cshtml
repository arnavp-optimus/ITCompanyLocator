@model List<CompLocatorUI.Models.CompanyModel>
@{
	ViewBag.Title = "DisplayOutput";
	
}

<h2 align="center">List of Companies</h2>


<body background="~/Images/back3.jpg" align="center">
	<table>
		<tr>
			<th>Name</th>
			<th>Address</th>
		</tr>

		@foreach (var value in Model)
		{
			<tr>
				<td> @value.Name </td>
				<td> @value.Address </td>
			</tr>
		}
	</table>
</body>

